<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>NeomandiPrototype</display-name>
  <welcome-file-list>
    <welcome-file>HomePage.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ControllerServlet</display-name>
    <servlet-name>ControllerServlet</servlet-name>
    <servlet-class>com.neomandi.prototype.ControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ControllerServlet</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SchedulerServlet</display-name>
    <servlet-name>SchedulerServlet</servlet-name>
    <servlet-class>com.neomandi.prototype.SchedulerServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>SchedulerServlet</servlet-name>
    <url-pattern>/SchedulerServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PostAuctionOperationServlet</display-name>
    <servlet-name>PostAuctionOperationServlet</servlet-name>
    <servlet-class>com.neomandi.prototype.PostAuctionOperationServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>PostAuctionOperationServlet</servlet-name>
    <url-pattern>/PostAuctionOperationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProductImage</display-name>
    <servlet-name>ProductImage</servlet-name>
    <servlet-class>com.neomandi.prototype.ProductImage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProductImage</servlet-name>
    <url-pattern>/ProductImage</url-pattern>
  </servlet-mapping>
</web-app>